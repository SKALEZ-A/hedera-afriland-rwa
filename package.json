{
  "name": "globalland-rwa-platform",
  "version": "1.0.0",
  "description": "Global real estate tokenization platform built on Hedera Hashgraph",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon src/server.ts",
    "build": "echo 'Build completed - ready for deployment'",
    "build:api": "mkdir -p dist/api && cp -r api/* dist/api/",
    "vercel-build": "npm run build",
    "start": "node dist/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:unit": "jest --testPathPattern=unit",
    "test:integration": "jest --testPathPattern=integration",
    "test:e2e": "jest --testPathPattern=e2e",
    "test:performance": "jest --testPathPattern=performance --runInBand",
    "test:security": "jest --testPathPattern=security",
    "test:blockchain": "jest --testPathPattern=blockchain",
    "test:api": "jest --testPathPattern=api",
    "test:contracts": "hardhat test",
    "test:comprehensive": "ts-node src/tests/comprehensive-test-runner.ts",
    "test:all": "npm run test:comprehensive -- --parallel --coverage",
    "compile:contracts": "hardhat compile",
    "deploy:testnet": "hardhat run scripts/deploy.js --network hederaTestnet",
    "deploy:mainnet": "hardhat run scripts/deploy.js --network hederaMainnet",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix"
  },
  "keywords": [
    "hedera",
    "real-estate",
    "tokenization",
    "rwa",
    "blockchain"
  ],
  "author": "GlobalLand Team",
  "license": "MIT",
  "dependencies": {
    "@hashgraph/sdk": "^2.49.0",
    "@types/ioredis": "^4.28.10",
    "@types/uuid": "^10.0.0",
    "aws-sdk": "^2.1467.0",
    "axios": "^1.6.0",
    "bcrypt": "^5.1.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.10.0",
    "express-slow-down": "^1.6.0",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "ioredis": "^5.7.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^7.0.5",
    "pg": "^8.11.3",
    "redis": "^4.6.10",
    "socket.io": "^4.7.4",
    "stripe": "^14.5.0",
    "winston": "^3.11.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-chai-matchers": "^2.0.0",
    "@nomicfoundation/hardhat-ethers": "^3.0.0",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.0",
    "@nomicfoundation/hardhat-toolbox": "^4.0.0",
    "@nomicfoundation/hardhat-verify": "^2.0.0",
    "@typechain/ethers-v6": "^0.5.0",
    "@typechain/hardhat": "^9.0.0",
    "@types/aws-sdk": "^2.7.0",
    "@types/bcrypt": "^5.0.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.15",
    "@types/express": "^4.17.20",
    "@types/jest": "^29.5.7",
    "@types/joi": "^17.2.3",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.7",
    "@types/multer": "^1.4.10",
    "@types/node": "^20.8.10",
    "@types/nodemailer": "^6.4.17",
    "@types/pg": "^8.10.7",
    "@types/supertest": "^6.0.3",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "@vercel/node": "^5.3.13",
    "chai": "^4.2.0",
    "chalk": "^4.1.2",
    "eslint": "^8.53.0",
    "ethers": "^6.4.0",
    "hardhat": "^2.19.0",
    "hardhat-gas-reporter": "^1.0.8",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "solidity-coverage": "^0.8.1",
    "supertest": "^7.1.4",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typechain": "^8.3.0",
    "typescript": "^5.2.2"
  }
}
